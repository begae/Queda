<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_feed">

    <fragment
        android:id="@+id/navigation_feed"
        android:name="com.whoasys.queda.FeedFragment"
        android:label="@string/title_feed"
        tools:layout="@layout/fragment_feed" >
        <action
            android:id="@+id/action_navigation_feed_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_recommend"
        android:name="com.whoasys.queda.RecommendFragment"
        android:label="@string/title_recommend"
        tools:layout="@layout/fragment_recommend" >
        <action
            android:id="@+id/action_navigation_recommend_to_storeProfileFragment"
            app:destination="@id/storeProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.whoasys.queda.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_map" >
        <action
            android:id="@+id/action_navigation_map_to_searchResultFragment"
            app:destination="@id/searchResultFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_my_page"
        android:name="com.whoasys.queda.MyPageFragment"
        android:label="@string/title_my_page"
        tools:layout="@layout/fragment_my_page" >
        <action
            android:id="@+id/action_navigation_my_page_to_scrapsFragment"
            app:destination="@id/scrapsFragment" />
        <action
            android:id="@+id/action_navigation_my_page_to_followingFragment"
            app:destination="@id/followingFragment" />
        <action
            android:id="@+id/action_navigation_my_page_to_firstKeywordFragment"
            app:destination="@id/firstKeywordFragment" />
        <action
            android:id="@+id/action_navigation_my_page_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinFragment"
        android:name="com.whoasys.queda.JoinFragment"
        android:label="fragment_join"
        tools:layout="@layout/fragment_join" >

        <action
            android:id="@+id/action_joinFragment_to_firstKeywordFragment"
            app:destination="@id/firstKeywordFragment" />
    </fragment>
    <fragment
        android:id="@+id/firstKeywordFragment"
        android:name="com.whoasys.queda.FirstKeywordFragment"
        android:label="fragment_first_keyword"
        tools:layout="@layout/fragment_first_keyword" >
        <action
            android:id="@+id/action_firstKeywordFragment_to_navigation_feed"
            app:destination="@id/navigation_feed" />
    </fragment>
    <fragment
        android:id="@+id/postFragment"
        android:name="com.whoasys.queda.PostFragment"
        android:label="fragment_post"
        tools:layout="@layout/fragment_post" >
        <action
            android:id="@+id/action_postFragment_to_storeProfileFragment"
            app:destination="@id/storeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchResultFragment"
        android:name="com.whoasys.queda.SearchResultFragment"
        android:label="fragment_search_result"
        tools:layout="@layout/fragment_search_result" >
        <action
            android:id="@+id/action_searchResultFragment_to_storeProfileFragment"
            app:destination="@id/storeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/scrapsFragment"
        android:name="com.whoasys.queda.ScrapsFragment"
        android:label="fragment_scraps"
        tools:layout="@layout/fragment_scraps" >
        <action
            android:id="@+id/action_scrapsFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/followingFragment"
        android:name="com.whoasys.queda.FollowingFragment"
        android:label="fragment_following"
        tools:layout="@layout/fragment_following" >
        <action
            android:id="@+id/action_followingFragment_to_storeProfileFragment"
            app:destination="@id/storeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.whoasys.queda.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_myStoreFragment"
            app:destination="@id/myStoreFragment" />
        <action
            android:id="@+id/action_registerFragment_to_verifyFragment"
            app:destination="@id/verifyFragment" />
    </fragment>
    <fragment
        android:id="@+id/myStoreFragment"
        android:name="com.whoasys.queda.MyStoreFragment"
        android:label="fragment_my_store"
        tools:layout="@layout/fragment_my_store" >
        <action
            android:id="@+id/action_myStoreFragment_to_postingFragment"
            app:destination="@id/postingFragment" />
        <action
            android:id="@+id/action_myStoreFragment_to_editStoreFragment"
            app:destination="@id/editStoreFragment" />
    </fragment>
    <fragment
        android:id="@+id/postingFragment"
        android:name="com.whoasys.queda.PostingFragment"
        android:label="fragment_posting"
        tools:layout="@layout/fragment_posting" >
        <action
            android:id="@+id/action_postingFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/verifyFragment"
        android:name="com.whoasys.queda.VerifyFragment"
        android:label="fragment_verify"
        tools:layout="@layout/fragment_verify" >
        <action
            android:id="@+id/action_verifyFragment_to_addressFragment"
            app:destination="@id/addressFragment" />
    </fragment>
    <fragment
        android:id="@+id/editStoreFragment"
        android:name="com.whoasys.queda.EditStoreFragment"
        android:label="fragment_edit_store"
        tools:layout="@layout/fragment_edit_store" >
        <action
            android:id="@+id/action_editStoreFragment_to_myStoreFragment"
            app:destination="@id/myStoreFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.whoasys.queda.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_joinFragment"
            app:destination="@id/joinFragment" />
        <action
            android:id="@+id/action_loginFragment_to_navigation_feed"
            app:destination="@id/navigation_feed" />
    </fragment>
    <fragment
        android:id="@+id/storeProfileFragment"
        android:name="com.whoasys.queda.StoreProfileFragment"
        android:label="fragment_store_profile"
        tools:layout="@layout/fragment_store_profile" >
        <action
            android:id="@+id/action_storeProfileFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/addressFragment"
        android:name="com.whoasys.queda.AddressFragment"
        android:label="fragment_address"
        tools:layout="@layout/fragment_address" >
        <action
            android:id="@+id/action_addressFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
</navigation>