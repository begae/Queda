<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/feedFragment">
    <fragment
        android:id="@+id/feedFragment"
        android:name="com.whoasys.queda.recyclers.FeedFragment"
        android:label="FeedFragment" >
        <action
            android:id="@+id/action_feedFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/postFragment"
        android:name="com.whoasys.queda.PostFragment"
        android:label="fragment_post"
        tools:layout="@layout/fragment_post" >
        <action
            android:id="@+id/action_postFragment_to_storeProfileFragment2"
            app:destination="@id/storeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeProfileFragment"
        android:name="com.whoasys.queda.StoreProfileFragment"
        android:label="fragment_store_profile"
        tools:layout="@layout/fragment_store_profile" />
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.whoasys.queda.api.MapFragment"
        android:label="MapFragment" >
        <action
            android:id="@+id/action_mapFragment_to_searchResultFragment"
            app:destination="@id/searchResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchResultFragment"
        android:name="com.whoasys.queda.recyclers.SearchResultFragment"
        android:label="SearchResultFragment" >
        <action
            android:id="@+id/action_searchResultFragment_to_storeProfileFragment"
            app:destination="@id/storeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.whoasys.queda.MyPageFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_my_page" >
        <action
            android:id="@+id/action_myPageFragment_to_scrapsFragment"
            app:destination="@id/scrapsFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_followingFragment"
            app:destination="@id/followingFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_editKeywordFragment"
            app:destination="@id/editKeywordFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_myStoreFragment"
            app:destination="@id/myStoreFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/scrapsFragment"
        android:name="com.whoasys.queda.recyclers.ScrapsFragment"
        android:label="ScrapsFragment" >
        <action
            android:id="@+id/action_scrapsFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/forYouFragment"
        android:name="com.whoasys.queda.recyclers.ForYouFragment"
        android:label="ForYouFragment" >
        <action
            android:id="@+id/action_forYouFragment_to_storeProfileFragment"
            app:destination="@id/storeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/followingFragment"
        android:name="com.whoasys.queda.recyclers.FollowingFragment"
        android:label="FollowingFragment" >
        <action
            android:id="@+id/action_followingFragment_to_storeProfileFragment"
            app:destination="@id/storeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/editKeywordFragment"
        android:name="com.whoasys.queda.recyclers.EditKeywordFragment"
        android:label="EditKeywordFragment" >
        <action
            android:id="@+id/action_editKeywordFragment_to_feedFragment"
            app:destination="@id/feedFragment" />
        <action
            android:id="@+id/action_editKeywordFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/myStoreFragment"
        android:name="com.whoasys.queda.MyStoreFragment"
        android:label="fragment_my_store"
        tools:layout="@layout/fragment_my_store" >
        <action
            android:id="@+id/action_myStoreFragment_to_postingFragment"
            app:destination="@id/postingFragment" />
    </fragment>
    <fragment
        android:id="@+id/joinFragment"
        android:name="com.whoasys.queda.JoinFragment"
        android:label="fragment_join"
        tools:layout="@layout/fragment_join" >
        <action
            android:id="@+id/action_joinFragment_to_editKeywordFragment"
            app:destination="@id/editKeywordFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.whoasys.queda.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_joinFragment"
            app:destination="@id/joinFragment" />
        <action
            android:id="@+id/action_loginFragment_to_feedFragment"
            app:destination="@id/feedFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.whoasys.queda.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_verifyFragment"
            app:destination="@id/verifyFragment" />
        <action
            android:id="@+id/action_registerFragment_to_addressFragment"
            app:destination="@id/addressFragment" />
        <action
            android:id="@+id/action_registerFragment_to_editKeywordFragment"
            app:destination="@id/editKeywordFragment" />
        <action
            android:id="@+id/action_registerFragment_to_myStoreFragment2"
            app:destination="@id/myStoreFragment" />
    </fragment>
    <fragment
        android:id="@+id/verifyFragment"
        android:name="com.whoasys.queda.api.VerifyFragment"
        android:label="VerifyFragment" >
        <action
            android:id="@+id/action_verifyFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/addressFragment"
        android:name="com.whoasys.queda.api.AddressFragment"
        android:label="AddressFragment" >
        <action
            android:id="@+id/action_addressFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/postingFragment"
        android:name="com.whoasys.queda.PostingFragment"
        android:label="fragment_posting"
        tools:layout="@layout/fragment_posting" >
        <action
            android:id="@+id/action_postingFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <navigation android:id="@+id/store_navigation"
        app:startDestination="@id/storeInfoFragment">
        <fragment
            android:id="@+id/storePromosFragment"
            android:name="com.whoasys.queda.recyclers.StorePromosFragment"
            android:label="StorePromosFragment" >
            <action
                android:id="@+id/action_storePromosFragment_to_postFragment"
                app:destination="@id/postFragment" />
        </fragment>
        <fragment
            android:id="@+id/storeInfoFragment"
            android:name="com.whoasys.queda.StoreInfoFragment"
            android:label="fragment_store_info"
            tools:layout="@layout/fragment_store_info" />
        <fragment
            android:id="@+id/storePostsFragment"
            android:name="com.whoasys.queda.recyclers.StorePostsFragment"
            android:label="StorePostsFragment" >
            <action
                android:id="@+id/action_storePostsFragment_to_postFragment"
                app:destination="@id/postFragment" />
        </fragment>
    </navigation>
</navigation>